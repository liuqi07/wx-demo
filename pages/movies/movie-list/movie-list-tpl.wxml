<import src="../movie/movie-tpl.wxml" />

<template name="movieListTpl">
    <view class="movie-list-container">
        <block wx:for="{{ movies }}" wx:for-item="movie">
            <view class="single-view-container">
                <template is="movieTpl" data="{{...movie}}"></template>
            </view>
        </block>
    </view>
</template>
