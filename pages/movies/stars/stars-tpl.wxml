<template name="starsTpl">
    <view class="stars-container">
        <view class="stars">
            <block wx:for="{{stars}}" wx:for-item="i">
                <image wx:if="{{i}}" src="/images/icon/star.png"></image>
                <image wx:else src="/images/icon/none-star.png"></image>
            </block>
        </view>
        <text>{{score}}</text>
    </view>
</template>